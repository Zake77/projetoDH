package projetodh;

public class arrayComIf {

	public static void main(String[] args) {
		
		
		String[] cores = new String[] {"amarelo", "azul", "roxo"};
		
		for (int i = 0; i < cores.length; i++) {
			if (i == 0) {
				System.out.println("Cor:" + cores[i] + "Posição" + i);
			}
		}
		
		

	}

}
